@{
    ViewBag.Title = "Home Page";
}

<div id="testimonials"></div>
<div class="container  text-center">
    <h2 >Shop Your Style</h2>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="item active">
                <div class="raw text-center">
                    <div class="col-md-4">
                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSKiTEMH9Obww3NZ2fyYsbTXi3o-qufdFue1g&usqp=CAU" alt="pic1" border="0" style="width:500px;height:500px">
                    </div>

                </div>
            </div>
            <div class="item">
                <div class="raw text-center">
                    <div class="col-md-4">
                        <img src="https://images.unsplash.com/photo-1516762689617-e1cffcef479d?ixid=MXwxMjA3fDB8MHxzZWFyY2h8NHx8ZmVuY3klMjBjbG90aGVzfGVufDB8fDB8&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="pic3" border="0" style="height:500px; width:500px">
                    </div>

                </div>
            </div>
            <div class="item">
                <div class="raw text-center">
                    <div class="col-md-4">
                        <img src="https://images.unsplash.com/photo-1575501265016-ae78c708a09c?ixid=MXwxMjA3fDB8MHxzZWFyY2h8Mnx8Y291Y2h8ZW58MHx8MHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="pic4" border="0" style="height:500px; width:500px">
                    </div>

                </div>
            </div>
            <div class="item">
                <div class="raw text-center">
                    <div class="col-md-4">
                        <img src="https://images.unsplash.com/photo-1534030347209-467a5b0ad3e6?ixid=MXwxMjA3fDB8MHxzZWFyY2h8ODV8fG1lbiUyMGNsb3Roc3xlbnwwfHwwfA%3D%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" alt="pic4" border="0" style="height:500px; width:500px">
                    </div>

                </div>
            </div>
            <div class="item">
                <div class="raw text-center">
                    <div class="col-md-4">
                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSToGkssqpvVHbAqzHwO2NbRW-_RNbvQjpuQQ&usqp=CAU" alt="pic4" border="0" style="height:500px; width:500px">
                    </div>

                </div>
            </div>

            <div>
                Start Slider Control
                <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>
</div>

